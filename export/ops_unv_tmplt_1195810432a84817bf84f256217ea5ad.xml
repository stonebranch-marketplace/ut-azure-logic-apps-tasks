    header_text = dict(response.headers)
    runName = header_text['x-ms-workflow-run-id']
    logging.info("Run ID is: "+runName)
        
    # Getting run output
    logging.info("Monitoring the run process")
    #url = 'https://management.azure.com/subscriptions/%(subscriptionid)s/resourceGroups/%(resourcegroupname)s/providers/Microsoft.Logic/workflows/%(workflowname)s/runs/%(runName)s?api-version=2016-06-01' % {'subscriptionid' : args.subscriptionid, 'resourcegroupname' : args.resourcegroupname, 'workflowname' : args.workflowname, 'run' : runName}
    url='https://management.azure.com/subscriptions/'+args.subscriptionid+'/resourceGroups/'+args.resourcegroupname+'/providers/Microsoft.Logic/workflows/'+args.workflowname+'/runs/'+runName+'?api-version=2016-06-01'
    print ("URL:",url)
    logging.info("Azure Logic workflow: "+args.workflowname+" Status is Running")
    while True:
        response = requests.get(url, headers=header)
