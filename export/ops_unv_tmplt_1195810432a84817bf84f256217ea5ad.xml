                job_start_time=format_exec_stat_res['properties']['startTime']
                job_end_time=format_exec_stat_res['properties']['endTime']
                output_logicapp=format_exec_stat_res['properties']['outputs']
                logging.info("Azure Logic workflow: "+args.workflowname+" completed with "+status_job)
                logging.info("Start Time: "+job_start_time+"        "+" End Time: "+job_end_time)
                #Set the status in the controller based on the logic app status
                if (status_job == "Succeeded"):
                    sys.exit(0)
                elif (status_job == "Failed"):
                    sys.exit(1)
                break
