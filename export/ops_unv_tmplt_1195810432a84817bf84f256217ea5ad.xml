    token="Bearer "+args.Azure_access_token
    #print("token",token)
    header={'Authorization':token,'content-type':args.requestformat, 'accept':args.responseformat}
    #Build the body for the post message
    post_body=args.param1name
    json_dumps=json.dumps(post_body)
    json_loads=json.loads(json_dumps)
    #print("dumps: ",json_dumps)
    print("loads:",json_loads)
    # Do the HTTP request
    try:
        logging.info('Attempting to run the following trigger: '+url)
        logging.info("Parameters for the Trigger: "+json_loads)
        response = requests.post(url,data=json_loads,headers=header)
        if response.status_code != 202: 
            sys.exit(1)        
        elif response.status_code == 202:
            logging.info("Workflow: "+ args.workflowname +" Triggered successfully in Logic Apps")
            #logging.info("Input_parameters supplied: "+data)
    except:
