def RunWorkflowTrigger():
    logging.info("Workflow Triggers - Run")
    # -- Set the request parameters
    url = 'https://management.azure.com/subscriptions/%(subscriptionid)s/resourceGroups/%(resourcegroupname)s/providers/Microsoft.Logic/workflows/%(workflowname)s/triggers/%(triggername)s/run?api-version=2016-06-01' % {'subscriptionid' : args.subscriptionid, 'resourcegroupname' : args.resourcegroupname, 'workflowname' : args.workflowname, 'triggername' : args.triggername}
